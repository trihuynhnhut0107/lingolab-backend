{
  "name": "lingolab-backend",
  "version": "1.0.0",
  "description": "LingoLab Backend API with Express.js, TypeORM, PostgreSQL, and Swagger",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsoa spec-and-routes && tsc",
    "dev": "npm run swagger && ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "swagger": "tsoa spec-and-routes",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/data-source.ts",
    "db:clear": "ts-node src/utils/clearDatabase.ts",
    "db:clear:confirm": "ts-node src/utils/clearDatabase.ts --confirm",
    "db:seed": "ts-node src/utils/seedDatabase.ts",
    "db:seed:confirm": "ts-node src/utils/seedDatabase.ts --confirm",
    "db:reset": "npm run db:clear:confirm && npm run db:seed:confirm"
  },
  "keywords": [
    "express",
    "typescript",
    "typeorm",
    "postgresql",
    "swagger",
    "tsoa"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/core": "^1.1.4",
    "@langchain/openai": "^1.1.3",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.3",
    "langchain": "^1.1.5",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.4",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
